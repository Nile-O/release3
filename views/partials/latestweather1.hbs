
<div class="ui five cards">

  <div class="card" style="background-color:#4183c4;">
    <h3>{{name}}</h3>
    <h5>Lat: {{latitude}}<br>
    Long: {{longitude}}</h5>
  </div>
  <div class="card" style="background-color:#4183c4;">
    <h3>Weather</h3>
    <br>
   <h5>{{weatherText}}</h5>

  </div>
  <div class="card" style="background-color:#4183c4;">
    <h3>Temperature</h3> <br>
   <h5> {{latestReading.temperature}} C <br>
    {{tempF}} F<br>
    Max: {{maxTemp}}<br>
    Min: {{minTemp}}<br>
    Trend: {{tempTrend}}</h5>

  </div>
  <div class="card" style="background-color:#4183c4;">
    <h3>Wind</h3> <br>
    <h5>{{latestReading.windSpeed}} bft <br>
    {{windText}}<br>
    Feels Like <br>
    {{windChill}}<br>
    Max: {{maxWind}}<br>
    Min: {{minWind}}<br>
      Trend: {{windTrend}} </h5>

  </div>
  <div class="card" style="background-color:#4183c4;">
    <h3>Pressure</h3> <br>
    <h5>{{latestReading.pressure}} hpa<br>
    Max: {{maxPressure}}<br>
    Min: {{minPressure}}<br>
    Trend: {{pressureTrend}}</h5>
  </div>
</div>
